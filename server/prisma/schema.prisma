// prisma/schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model TokenLookup {
  id        Int      @id @default(autoincrement())
  token     String
  score     Int
  risk      String
  outlook   String
  createdAt DateTime @default(now())

  @@index([createdAt])
  @@index([token, createdAt])
}
